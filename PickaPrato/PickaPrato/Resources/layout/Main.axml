<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:orientation="vertical"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/linearLayout1"
    p1:background="@drawable/background">
    <ImageView
        p1:src="@drawable/logo"
        p1:layout_width="299.0dp"
        p1:layout_height="208.0dp"
        p1:id="@+id/imageView1"
        p1:layout_marginBottom="0.0dp"
        p1:layout_gravity="center" />
    <EditText
        p1:id="@+id/username"
        p1:layout_width="299.0dp"
        p1:layout_height="wrap_content"
        p1:hint="Username"
        p1:layout_marginRight="0.0dp"
        p1:layout_gravity="center" />
    <EditText
        p1:inputType="textPassword"
        p1:layout_width="299.0dp"
        p1:layout_height="wrap_content"
        p1:hint="Password"
        p1:layout_marginRight="0.0dp"
        p1:layout_gravity="center" />
    <Space
        p1:layout_width="match_parent"
        p1:layout_height="43.5dp"
        p1:id="@+id/space1"
        p1:layout_marginBottom="0.0dp" />
    <Button
        p1:id="@+id/iniciarsessao"
        p1:text="INICIAR SESSÃƒO"
        p1:layout_width="156.0dp"
        p1:layout_height="wrap_content"
        p1:layout_gravity="center" />
    <Space
        p1:layout_width="match_parent"
        p1:layout_height="106.5dp"
        p1:id="@+id/space2" />
    <Button
        p1:id="@+id/registar"
        p1:text="REGISTAR"
        p1:layout_width="156.0dp"
        p1:layout_height="wrap_content"
        p1:layout_gravity="center" />
</LinearLayout>